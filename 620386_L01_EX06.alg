
inicio
    real salario, salario_receber, salario_final
    
    escrever "Entre com seu salario: "
    Ler Salario
    
    salario_receber <- salario * 1.5
    
    escrever "Seu salario com aumento de 5% é: ", salario_receber
    
    salario_final <- (salario_receber * 1.7) - salario_receber
    
    Escrever "\nEste é seu salario final com desconto de 7% de impostos: ", salario_final
fim
