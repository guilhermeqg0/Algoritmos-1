
inicio
    REAL altura_degrau, altura_desejada, degraus_subir, degraus_escada
    
    ESCREVER "Informe a altura do degrau: "
    LER altura_degrau
    
    ESCREVER "Informe a altura desejada à alcancar: "
    LER altura_desejada
        
    degraus_subir <- altura_desejada / altura_degrau
    
    ESCREVER "O número de degraus a subir é: ", degraus_subir
    
    
	
fim
